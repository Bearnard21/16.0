<odoo noupdate="0">
    <record id="website.default_website" model="website">
        <field name="theme_id" ref="base.module_theme_website_cetmix" />
        <field
            name="logo"
            type="base64"
            file="theme_website_cetmix/static/img/header_1_1.svg"
        />
    </record>

    <!-- Pages -->
    <record id="cetmix" model="website.page">
        <field name="url">/why_cetmix</field>
        <field name="is_published">True</field>
        <field name="view_id" ref="why_cetmix" />
        <field name="track">True</field>
    </record>

    <record id="odoo" model="website.page">
        <field name="url">/why_odoo</field>
        <field name="is_published">True</field>
        <field name="view_id" ref="why_odoo" />
        <field name="track">True</field>
    </record>

    <record id="solutions" model="website.page">
        <field name="url">/our_solutions</field>
        <field name="is_published">True</field>
        <field name="view_id" ref="our_solutions" />
        <field name="track">True</field>
    </record>

    <!-- Menus -->
    <record id="menu_cetmix_why_cetmix" model="website.menu">
        <field name="name">Why Cetmix?</field>
        <field name="url">/why_cetmix</field>
        <field name="page_id" ref="cetmix" />
        <field name="parent_id" ref="website.main_menu" />
        <field name="sequence" type="int">60</field>
    </record>

    <record id="menu_cetmix_why_odoo" model="website.menu">
        <field name="name">Why Odoo?</field>
        <field name="url">/why_odoo</field>
        <field name="page_id" ref="odoo" />
        <field name="parent_id" ref="website.main_menu" />
        <field name="sequence" type="int">61</field>
    </record>

    <record id="menu_cetmix_our_solutions" model="website.menu">
        <field name="name">Our Solutions</field>
        <field name="url">/our_solutions</field>
        <field name="page_id" ref="solutions" />
        <field name="parent_id" ref="website.main_menu" />
        <field name="sequence" type="int">62</field>
    </record>



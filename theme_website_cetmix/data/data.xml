<odoo noupdate="1">
    <record id="website.default_website" model="website">
        <field name="theme_id" ref="base.module_theme_website_cetmix" />
        <field
            name="logo"
            type="base64"
            file="theme_website_cetmix/static/img/header_1_1.svg"
        />
    </record>


    <!-- Pages -->
    <record id="about" model="website.page">
        <field name="url">/why_cetmix</field>
        <field name="is_published">True</field>
        <field name="view_id" ref="why_cetmix"/>
        <field name="track">True</field>
    </record>

    <!-- Menus -->
    <record id="menu_cetmix_why_cetmix" model="website.menu">
        <field name="name">Why Cetmix?</field>
        <field name="url">/why_cetmix</field>
        <field name="page_id" ref="about" />
        <field name="parent_id" ref="website.main_menu"/>
        <field name="sequence" type="int">60</field>
    </record>

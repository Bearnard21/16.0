<odoo>
    <record id="website.default_website" model="website">
        <field name="theme_id" ref="base.module_theme_website_cetmix" />
        <field
            name="logo"
            type="base64"
            file="theme_website_cetmix/static/img/header_1_1.svg"
        />
    </record>

    <!-- Pages -->
    <record id="cetmix" model="theme.website.page">
        <field name="url">/why_cetmix</field>
        <field name="is_published">True</field>
        <field name="view_id" ref="why_cetmix" />
    </record>

    <record id="odoo" model="theme.website.page">
        <field name="url">/why_odoo</field>
        <field name="is_published">True</field>
        <field name="view_id" ref="why_odoo" />
    </record>

    <record id="solutions" model="theme.website.page">
        <field name="url">/our_solutions</field>
        <field name="is_published">True</field>
        <field name="view_id" ref="our_solutions" />
    </record>

    <!-- Menus -->
    <record id="menu_cetmix_why_cetmix" model="theme.website.menu">
        <field name="name">Why Cetmix?</field>
        <field name="url">/why_cetmix</field>
        <field name="sequence" type="int">22</field>
    </record>
    <record id="menu_cetmix_why_odoo" model="theme.website.menu">
        <field name="name">Why Odoo?</field>
        <field name="url">/why_odoo</field>
        <field name="sequence" type="int">23</field>
    </record>
    <record id="menu_cetmix_our_solutions" model="theme.website.menu">
        <field name="name">Our Solutions</field>
        <field name="url">/our_solutions</field>
        <field name="sequence" type="int">24</field>
    </record>

